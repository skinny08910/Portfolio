{
  "name": "listener",
  "version": "1.0.0",
  "description": "A static music site.",
  "main": "index.js",
  "scripts": {
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.concat.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skinny08910/listener.git"
  },
  "keywords": [
    "music",
    "rap",
    "band",
    "classic",
    "music"
  ],
  "author": "Nicksonder Examar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/skinny08910/listener/issues"
  },
  "homepage": "https://github.com/skinny08910/listener#readme",
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "concat": "^1.0.3",
    "live-server": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "post-cli": "^1.0.5"
  }
}
